                <%= fp.dh_reduced / fp.fight_time %>/s
              </div>
          </div>
          <div class="col-md-3"><%= fp.dh_reduced / 1000 %>k mitigated</div>
        </div>
        <% fp.cooldown_parses.dh.order(id: :asc).each do |dh| %>
          <div class="row" onclick="$('#dh-<%= dh.id %>').slideToggle();">
            <div class="col-md-2 text-right"><%= dh.time_s %> <img src="/assets/down.png" height="15px" width="15px"/></div>
            <div class="col-md-5">
              <div class="bar p<%= i %>" style="width: <%= 100 * dh.reduced_amount / @max_dh %>%"><%= dh.reduced_amount / 1000 %>k</div>
            </div>
            <div class="col-md-3"><%= dh.reduced_amount %> dmg / <%= dh.time %> sec</div>
          </div>
          <div id="dh-<%= dh.id %>" class="row slideout">
            <div class="col-md-2">&nbsp;</div>
            <div class="col-md-5">
              <table style="width: 100%">
                <thead><tr><th>Ability</th><th>Incoming damage</th><th>% Max HP</th></tr></thead>
                <tbody>
                  <% dh.ability_hash.each do |ability_id, hash| %>
                    <tr>
                      <td><%= hash[:name] %></td>
                      <td><%= hash[:amount] %></td>
                      <td><%= (hash[:percent] * 200).round(2) %></td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
          </div>
        <% end %>
      </section>
    <% end %>
  <% end %>

