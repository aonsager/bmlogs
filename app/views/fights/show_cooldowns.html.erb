<% breadcrumb :fight, @fight, @fps.first, @report %>

<div class="container">
  <div class="title"><%= "#{@fight.name} (#{@fps.first.fight_time / 60}:#{@fps.first.fight_time % 60})" %></div>
  <% @fps.each_with_index do |fp, i| %>
    <div class="player-tag p<%= i %>"><%= UserToPlayer.find_by(player_id: fp.player_id).player_name %></div>
  <% end %>
  <div style="clear:both"></div>
  <%= link_to('Key Metrics', '?tab=basic', class: 'btn btn-default') %>
  <%= link_to('Resource Management', '?tab=resources', class: 'btn btn-default') %>
  <%= link_to('Cooldown Usage', '?tab=cooldowns', class: 'btn btn-primary') %>
  <br /><br />

  <h4>Guard Effectiveness</h4>
  <% @fps.each_with_index do |fp, i| %>
    <section>
      <div class="row">
        <div class="col-md-7">
          <div class="bar p<%= i %> light" style="width: <%= 100 * fp.gps / @max_bar %>%">
            <div class="bar p<%= i %>" style="width: <%= 100 * fp.guard_healed / (fp.guard_absorbed + fp.guard_healed) %>%">
              <%= fp.gps %>/s
            </div>
          </div>
        </div>
        <div class="col-md-3"><%= fp.guard_healed / 1000 %>k healed, <%= fp.guard_absorbed / 1000 %>k absorbed</div>
      </div>
      <% fp.cooldown_parses.guard.order(id: :asc).each do |guard| %>
        <div class="row" onclick="$('#g-<%= guard.id %>').slideToggle();">
        <div class="col-md-2 text-right"><%= guard.time_s %> <img src="/assets/down.png" height="15px" width="15px"/></div>
        <div class="col-md-5">
          <div class="bar p<%= i %> light" style="width: <%= 100 * (guard.absorbed_amount + guard.healed_amount) / @max_guard %>%">
            <div class="bar p<%= i %>" style="width: <%= 100 * guard.healed / (guard.absorbed_amount + guard.healed_amount) rescue 0 %>%"><%= (guard.absorbed_amount + guard.healed_amount) / 1000 %>k</div>
          </div>
        </div>
        <div class="col-md-3"><%= guard.healed_amount / 1000 %>k healed, <%= guard.absorbed_amount / 1000 %>k absorbed</div>
      </div>
      <div id="g-<%= guard.id %>" class="row slideout">
          <div class="col-md-2">&nbsp;</div>
          <div class="col-md-5">
            <table style="width: 100%">
              <thead><tr><th>Ability</th><th>Casts</th><th>Incoming Damage</th></tr></thead>
              <tbody>
                <% guard.ability_hash.each do |ability_id, hash| %>
                  <tr>
                    <td><%= hash[:name] %></td>
                    <td><%= hash[:casts] %></td>
                    <td><%= hash[:amount] %></td>
                  </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      <% end %>
    </section>
  <% end %>
  

  <h4>Elusive Brew Effectiveness</h4>
  <% @fps.each_with_index do |fp, i| %>
    <section>
      <div class="row">
        <div class="col-md-7">
            <div class="bar p<%= i %>" style="width: <%= 100 * fp.ebps / @max_bar %>%">
              <%= fp.ebps %>/s
            </div>
        </div>
        <div class="col-md-3"><%= fp.eb_avoided / 1000 %>k avoided</div>
      </div>
      <% fp.cooldown_parses.eb.order(id: :asc).each do |eb| %>
        <div class="row" onclick="$('#eb-<%= eb.id %>').slideToggle();">
          <div class="col-md-2 text-right"><%= eb.time_s %> <img src="/assets/down.png" height="15px" width="15px"/></div>
          <div class="col-md-5">
            <div class="bar p<%= i %>" style="width: <%= 100 * eb.reduced_amount / @max_eb %>%"><%= eb.reduced_amount / 1000 %>k</div>
          </div>
          <div class="col-md-3"><%= eb.reduced_amount %> dmg / <%= eb.time %> sec</div>
        </div>
        <div id="eb-<%= eb.id %>" class="row slideout">
          <div class="col-md-2">&nbsp;</div>
          <div class="col-md-5">
            <table style="width: 100%">
              <thead><tr><th>Ability</th><th>Dodged Count</th><th>Avg. dmg.</th></tr></thead>
              <tbody>
                <% eb.ability_hash.each do |source_id, source_hash| %>
                  <% source_hash[:abilities].each do |ability_id, hash| %>
                    <tr>
                      <td><%= hash[:name] %></td>
                      <td><%= hash[:dodged] %></td>
                      <td><%= hash[:avg] %></td>
                    </tr>
                  <% end %>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      <% end %>
    </section>
  <% end %>

  <% unless @fps.map{|fp| fp.cooldown_parses.dh.count}.max == 0 %>
    <h4>Dampen Harm Effectiveness</h4>
    <% @fps.each_with_index do |fp, i| %>
      <section>
        <div class="row">
          <div class="col-md-1"></div>
